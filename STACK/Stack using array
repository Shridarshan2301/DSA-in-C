#include <stdio.h>
int arr[100], top = -1, n = 100;
void push(int val) {
    if (top >= n - 1) {
        printf("Stack Overflow\n");
    } else {
        arr[++top] = val;
        printf("%d pushed to stack\n", val);
    }
}
int pop() {
    if (top < 0) {
        printf("Stack Underflow\n");
        return -1;
    } else {
        return arr[top--];
    }
}
int peek() {
    if (top < 0) {
        printf("Stack is empty\n");
        return -1;
    } else {
        return arr[top];
    }
}
int isEmpty() {
    return top < 0;
}
void display() {
    if (top < 0) {
        printf("Stack is empty\n");
    } else {
        printf("Stack elements:\n");
        for (int i = top; i >= 0; i--) {
            printf("%d\n", arr[i]);
        }
    }
}
int main() {
    push(10);
    push(20);
    push(30);
    display();
    printf("%d popped from stack\n", pop());
    printf("Top element is %d\n", peek());
    display();
    return 0;
}
